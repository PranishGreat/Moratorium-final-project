<!DOCTYPE html>
<html lang="en">
  
  <!-- Include Head Section which consits all the links of css -->
  <%- include('admin_head_section.ejs') %>

<body>
  <!-- container section start -->
  <section id="container" class="">
    
    <!--header start-->
    <%- include('admin_header_section.ejs') %>
    <!--header end-->

    <!--sidebar start-->
    <%- include('sidebar_section.ejs') %>
    <!--sidebar end-->

    <!--main content start-->
    <section id="main-content">
        <section class="wrapper">
          <div class="row">
            <div class="col-lg-12">
              <h3 class="page-header"><i class="fa fa-table"></i> Contact</h3>
            </div>
          </div>
          <!-- page start-->
          <div class="row">
            <div class="col-lg-12">
              <section class="panel">
                <header class="panel-heading">
                  Advanced Table
                </header>
                <% if(contact!=null) { %>
                <table class="table table-striped table-advance table-hover">
                    <tr>
                      <th><i class="icon_profile"></i> Sr. No. </th>
                      <th><i class="icon_profile"></i> Name </th>
                      <th><i class="icon_profile"></i> Email </th>
                      <th><i class="icon_profile"></i> Subject </th>
                      <th><i class="icon_calendar"></i> Message </th>
                    </tr>
                    <% var i=1; contact.forEach(function(item){ %>
                    <tr>
                      <td><%= i %></td>
                      <!-- <td><%= i %></td> -->
                      <td><%= item.contact_name %></td>
                      <td><%= item.contact_email %></td>
                      <td><%= item.contact_subject %></td>
                      <td><%= item.contact_message %></td>
                    </tr>
                    <% i++; }) %>
                </table>
                <% } %>
              </section>
            </div>
          </div>
          <!-- page end-->
        </section>
    </section>
    <!--main content end-->
  </section>
  <!-- container section end -->
  
  <!-- Include script_links Section which consits all the links of js and js scripts -->
  <%- include('script_links.ejs') %>

</body>
</html>
